Eddroid.Preload = function(game) {
  this.ready = false;

};


Eddroid.Preload.prototype = {

  preload: function() {
    this.load.image('premap', '<%= asset_path('premap.png') %>');
    this.load.image('logo', '<%= asset_path('logo.png') %>');
    this.load.tilemap('mapjson', '<%= asset_path('STREET.json') %>', null, Phaser.Tilemap.TILED_JSON);
    this.load.image('map', '<%= asset_path('STREET.png') %>');
    this.load.spritesheet('EDDROIDfront', '<%= asset_path('EDDROIDfront.png')%>', 32, 32, 7);

  },

  create: function() {
  },

  update: function() {
    this.ready = true;
    this.state.start('Menu');
  }
};
