Eddroid.Preload = function(game) {
  this.ready = false;

};


Eddroid.Preload.prototype = {

  preload: function() {
    this.load.image('premap', '<%= asset_path('premap.png') %>');
    this.load.image('logo', '<%= asset_path('logo.png') %>');
    this.load.tilemap('block', '<%= asset_path('collision.json') %>', null, Phaser.Tilemap.TILED_JSON);
    this.load.image('thecity', '<%= asset_path('thecity.png') %>');
  },

  create: function() {
  },

  update: function() {
    this.ready = true;
    this.state.start('Menu');
  }
};
